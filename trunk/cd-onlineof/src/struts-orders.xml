<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<package name="orders" extends="interceptor-default">
		<action name="searchOrders" class="ordersAction" method="searchOrders">
			<result name="success">/administrator/order/ordersManager.jsp</result>
		</action>
		<action name="getInfo" class="ordersAction" method="searchOrderInfo">
			<result name="success">/administrator/order/ordersEdit.jsp</result>
		</action>
		<action name="update" class="ordersAction" method="updateOrders">
			<result name="success">/administrator/order/ordersEdit.jsp</result>
		</action>
		<action name="updateMany" class="ordersAction" method="updateManyOrders">
			<result name="success" type="redirect">
				searchOrders.do?o=${o}
			</result>
		</action>
		<action name="del" class="ordersAction" method="delOrders">
			<result name="success" type="redirect">
				searchOrders.do?o=${o}
			</result>
		</action>
		<action name="add" class="ordersAction" method="addOrders">
			<result name="success" type="redirect">
				searchOrders.do?o=${o}
			</result>
		</action>
	</package>
</struts>